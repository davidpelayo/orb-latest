{
  "name": "orb-latest",
  "version": "4.0.0",
  "license": "MIT",
  "description": "Pivot table javascript library",
  "author": "David Pelayo <hi@davidpelayo.com>",
  "homepage": "http://orbjs.net/",
  "repository": {
    "type": "git",
    "url": "https://github.com/davidpelayo/orb-latest.git"
  },
  "bugs": {
    "url": "https://github.com/davidpelayo/orb-latest/issues"
  },
  "keywords": [
    "pivot table",
    "pivot",
    "table",
    "pivot grid",
    "grid",
    "crosstab",
    "aggregation",
    "dimension"
  ],
  "main": "dist/orb.js",
  "module": "dist/orb.esm.js",
  "types": "dist/orb.d.ts",
  "files": [
    "README.md",
    "LICENSE",
    "dist/*.js",
    "dist/*.css",
    "dist/*.map",
    "dist/*.d.ts",
    "src/"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-jest": "^30.2.0",
    "create-react-class": "^15.7.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "less": "^4.2.1",
    "lint-staged": "^15.2.11",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.28.1",
    "rollup-plugin-postcss": "^4.0.2",
    "vite": "^6.0.3"
  },
  "scripts": {
    "dev": "vite",
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,less}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,less}\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,less,md}": [
      "prettier --write"
    ]
  }
}
